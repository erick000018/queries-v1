                MaeEstadoDelitosDet estadoDelitoArchivado = maeEstadoDelitoDetRepository.findById(Constant.ARCHIVADO).orElseThrow(() -> new RuntimeException(Constant.TEXTO_ESTADO_NO_ENCONTRADO));
